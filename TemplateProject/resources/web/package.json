{
  "name": "templateproject-web-ui",
  "version": "1.0.0",
  "description": "TemplateProject Web UI",
  "scripts": {
    "build": "tsc && NODE_ENV=production esbuild scripts/index.js --bundle --format=iife --global-name=TemplateProjectUI --outfile=scripts/index.bundle.js --jsx=automatic --minify --define:process.env.NODE_ENV=\\\"production\\\"",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "esbuild": "^0.19.12",
    "typescript": "^5.3.0"
  }
}
